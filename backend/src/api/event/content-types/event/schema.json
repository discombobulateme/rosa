{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Events",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "eventname": {
      "type": "string",
      "required": true,
      "default": "eventname",
      "unique": true
    },
    "description": {
      "type": "richtext"
    },
    "opening": {
      "type": "datetime"
    },
    "finish": {
      "type": "datetime"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "artists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::artist.artist",
      "inversedBy": "events"
    },
    "sponsor": {
      "type": "string"
    },
    "sponsorlogo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
